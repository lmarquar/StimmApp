{
	"functions": [
		{
			"source": "functions",
			"codebase": "default",
			"ignore": [
				"node_modules",
				".git",
				"firebase-debug.log",
				"firebase-debug.*.log"
			],
			"predeploy": [
				"cd functions && ./node_modules/.bin/tsc"
			]
		}
	],
	"firestore": {
		"rules": "firestore.rules"
	},
	"storage": {
		"rules": "storage.rules"
	},
	"flutter": {
		"platforms": {
			"android": {
				"default": {
					"projectId": "stimmapp-f0141",
					"appId": "1:223242419578:android:05c6b7df0cf5ec3488aac3",
					"fileOutput": "android/app/google-services.json"
				}
			},
			"dart": {
				"lib/firebase_options.dart": {
					"projectId": "stimmapp-f0141",
					"configurations": {
						"android": "1:223242419578:android:05c6b7df0cf5ec3488aac3",
						"ios": "1:223242419578:ios:3e93442161af012588aac3"
					}
				}
			}
		}
	},
	"hosting": {
		"public": "build/web",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [
			{
				"source": "/delete-account.html",
				"destination": "/delete-account.html"
			},
			{
				"source": "**",
				"destination": "/index.html"
			}
		]
	}
}